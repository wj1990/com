page{
  width:100%;
  height:100%;
}
.enter-wrap{
  width:100%;
  height:100%;
  background: #b3d4db;
}
.enter-img{
  width: 160rpx;
  height: 160rpx;
  display: block;
  padding: 137rpx 0 95rpx;
  margin: 0 auto;
}
.enter-title{
  height: 48rpx;
  line-height: 48rpx;
  color: #000;
  display: block;
  text-align: center;
  font-size: 38rpx;
  margin-bottom: 198rpx;
}
.navigator-hover{
  width: 250rpx;
  height: 78rpx;
  margin: 0 auto;
  display: block;
}
.enter-text{
  width: 250rpx;
  height: 78rpx;
  line-height: 78rpx;
  text-align: center;
  border: 1px solid #405f80;
  border-radius: 10rpx;
  display: block;
  color: #405f80;
  display: block;
  text-align: center;
  font-size: 30rpx;
  margin: 0 auto;
}